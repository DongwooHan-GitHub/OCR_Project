def make_food_list():
    for row in file.readlines():
        if row[nutrition] != '':
            res[nutrition].append(row['대표식품명'])
            
    return res

def get_food_category_from_nutrition(food_list, nutrition_list):
    res = []
    
    for nutrition in nutrition_list:
        res.extend(food_list[nutrition])
        
    return list(set(res))